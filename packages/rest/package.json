{
  "name": "ts-fetcher",
  "module": "true",
  "version": "1.1.0",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "format:fix": "npx prettier --write \"src/**/*.ts\"",
    "format:check": "npx prettier --check \"src/**/*.ts\"",
    "lint:check": "npx eslint src --ext .ts",
    "lint:fix": "npx eslint src --ext .ts --fix",
    "build": "npx tsc --build",
    "pipeline": "npm run format:fix && bun run lint:fix && npm run build"
  },
  "repository": {
    "url": "https://github.com/Ayomits/ts-fetch"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "@types/node": "^24.0.12",
    "@typescript-eslint/eslint-plugin": "^8.36.0",
    "@typescript-eslint/parser": "^8.36.0",
    "eslint": "^9.30.1",
    "ioredis": "^5.6.1",
    "prettier": "^3.6.2",
    "typescript": "^5.8.3"
  },
  "files": [
    "dist"
  ],
  "author": "Ayomits",
  "type": "module"
}
