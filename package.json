    "release": "changeset publish",
    "test": "vitest"
  },
  "devDependencies": {
    "@changesets/cli": "^2.29.5",
    "@types/node": "^24.0.13",
    "@vitest/coverage-v8": "3.2.4",
    "tsc-alias": "^1.8.16",
    "tsup": "^8.5.0",
    "turbo": "^2.5.4",
    "typescript": "^5.8.3",
    "vitest": "^3.2.4"
