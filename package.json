{
  "version": "1.0.0",
  "description": "Typescript module for comfortable fetch",
  "private": true,
  "keywords": [
    "fetch",
    "nodejs",
    "typescript",
    "api"
  ],
  "homepage": "https://github.com/Ayomits/ts-fetch#readme",
  "bugs": {
    "url": "https://github.com/Ayomits/ts-fetch/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Ayomits/ts-fetch.git"
  },
  "license": "MIT",
  "author": "Ayomits",
  "type": "commonjs",
  "main": "index.js",
  "packageManager": "pnpm@10.12.1",
  "scripts": {
    "build": "npx turbo build --concurrency=4",
    "docs:dev": "npx turbo dev --concurrency=4",
    "format:fix": "npx turbo format:fix --concurrency=4",
    "lint:fix": "npx turbo lint:fix --concurrency=4",
    "format:check": "npx turbo format:check --concurrency=4",
    "lint:check": "npx turbo lint:check --concurrency=4",
